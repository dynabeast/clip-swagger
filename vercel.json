{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs/(.*)",
            "dest": "/api-docs/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}